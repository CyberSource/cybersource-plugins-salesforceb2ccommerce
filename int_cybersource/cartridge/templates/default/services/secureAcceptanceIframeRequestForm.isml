<html>
<head>
	<title>${Resource.msg('sa.iframe.form.title','cybersource',null)}</title>
</head>
<body>
	<style>
	.container {
	    background-color: #000000;
	    }
	</style>
	<form  style="display:none" name="form_iframe" method="post" action="${pdict.formAction}"> 
		<isif condition="${!empty(pdict.requestData)}"> 
			<isloop items="${pdict.requestData.keySet().iterator()}" var="addresskey">
				<input type="hidden" id="${addresskey}" name="${addresskey}" value='<isprint value="${pdict.requestData.get(addresskey)}" encoding="off"/>' />
			</isloop>
		</isif>
	</form>
	 
	<script type="text/javascript">
	document.form_iframe.submit(); 
	</script>
	
</body>
</html>