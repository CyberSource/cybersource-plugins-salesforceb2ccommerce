<style type="text/css">
label {
	width:300px;    /*Or however much space you need for the formâ€™s labels*/
    float:left;
}
td {
	border-width: medium; 
	border-style: outset;
}
</style>
<iscomment>
	This template will populate Test result of reversal.
</iscomment>
<label><b>CyberCource Decision :</b></label><isprint value="${pdict.reversalResponse.Decision}"><br>
<label><b>CyberSource Reason Code :</b></label><isprint value="${pdict.reversalResponse.ReasonCode}"><br>
<label><b>CyberSource RequestId :</b></label><isprint value="${pdict.reversalResponse.RequestID}"><br>
<label><b>CyberSource Request Token :</b></label><isprint value="${pdict.reversalResponse.RequestToken}"><br/>
<label><b>Amount :</b></label><isprint value="${pdict.reversalResponse.ReversalAmount}"><br/>
<label><b>Authorization Code :</b></label><isprint value="${pdict.reversalResponse.authorizationCode}"><br/>
<label><b>Processor Response :</b></label><isprint value="${pdict.reversalResponse.processorResponse}"><br/><br/><br/>
<label><b>CyberSource Response for Invalid Field :</b></label>
<isloop items="${pdict.reversalResponse.invalidField}" alias="invalidField">									
	<isprint value="${invalidField}">,
</isloop>
<br>
<label><b>CyberSource Response for Missing Field :</b></label>
<isloop items="${pdict.reversalResponse.missingField}" alias="missingField">									
	<isprint value="${missingField}">,
</isloop>
<br>

