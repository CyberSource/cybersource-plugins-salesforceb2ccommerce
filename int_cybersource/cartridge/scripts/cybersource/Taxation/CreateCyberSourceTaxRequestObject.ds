/**
* CreateCyberSourceTaxRequestObject.ds
* This script creates the Tax Request Object and set it to CybersourceTaxService output object.
*
* @output CybersourceTaxService : Object
*
*/
var TaxHelper = require('int_cybersource/cartridge/scripts/Helper/TaxHelper');

function execute( pdict : PipelineDictionary ) : Number
{
	pdict.CybersourceTaxService = TaxHelper.CreateCyberSourceTaxRequestObject().taxRequestObject;        
    return PIPELET_NEXT;
}
