            <attribute-definition attribute-id="CsDavEnable">
                <display-name xml:lang="x-default">Cybersource - Enable Delivery Address Verification</display-name>
                <description xml:lang="x-default">This will enable Delivery Address Verification, to help minimize risk of undeliverable or returns orders, because of user data entry errors.</description>
                <type>enum-of-string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <value-definitions>
                    <value-definition default="true">
                        <display xml:lang="x-default">YES</display>
                        <value>YES</value>
                    </value-definition>
                    <value-definition>
                        <display xml:lang="x-default">NO</display>
                        <value>NO</value>
                    </value-definition>
                </value-definitions>
            </attribute-definition>
            
            <attribute-definition attribute-id="CsDavOnAddressVerificationFailure">
                <display-name xml:lang="x-default">Cybersource - On Delivery Address Verification Failure</display-name>
                <description xml:lang="x-default">Prevent/enable authorization of payment if the DeliveryAddressVerification results in an error or rejection response.</description>
                <type>enum-of-string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <value-definitions>
                    <value-definition default="true">
                        <display xml:lang="x-default">DECLINE</display>
                        <value>DECLINE</value>
                    </value-definition>
                    <value-definition>
                        <display xml:lang="x-default">APPROVE</display>
                        <value>APPROVE</value>
                    </value-definition>
                </value-definitions>
            </attribute-definition>
