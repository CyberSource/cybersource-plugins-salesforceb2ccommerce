<?xml version="1.0" encoding="UTF-8"?>
<payment-settings xmlns="http://www.demandware.com/xml/impex/paymentsettings/2009-09-15">
    <payment-method method-id="SA_IFRAME">
        <name xml:lang="x-default">Credit Card - Secure Acceptance Web/Mobile (Iframe)</name>
        <description xml:lang="x-default">Secure Acceptance IFrame</description>
        <enabled-flag>true</enabled-flag>
        <processor-id>CYBERSOURCE_CREDIT</processor-id>
    </payment-method>

    <payment-method method-id="SA_SILENTPOST">
        <name xml:lang="x-default">Credit Card - Secure Acceptance Silent Order POST</name>
        <enabled-flag>true</enabled-flag>
        <processor-id>CYBERSOURCE_CREDIT</processor-id>
    </payment-method>

    <payment-method method-id="CREDIT_CARD">
        <name xml:lang="x-default">Credit Card</name>
        <name xml:lang="fr-FR">Carte de crédit</name>
        <name xml:lang="it-IT">Carta di credito</name>
        <name xml:lang="ja-JP">クレジットカード</name>
        <name xml:lang="zh-CN">信用卡</name>
        <enabled-flag>true</enabled-flag>
        <processor-id>CYBERSOURCE_CREDIT</processor-id>
    </payment-method>

    <payment-method method-id="ALIPAY">
        <name xml:lang="x-default">AliPay</name>
        <enabled-flag>true</enabled-flag>
        <processor-id>CYBERSOURCE_ALIPAY</processor-id>
    </payment-method>

    <payment-method method-id="PayPal">
        <name xml:lang="x-default">PayPal</name>
        <name xml:lang="fr-FR">Pay Pal</name>
        <name xml:lang="it-IT">PayPal</name>
        <name xml:lang="ja-JP">PayPal</name>
        <name xml:lang="zh-CN">PayPal</name>
        <enabled-flag>true</enabled-flag>
        <processor-id>PAYPAL_EXPRESS</processor-id>
    </payment-method>

    <payment-method method-id="DW_APPLE_PAY">
        <name xml:lang="x-default">Apple Pay</name>
        <enabled-flag>false</enabled-flag>
        <processor-id>BASIC_CREDIT</processor-id>
    </payment-method>

    <payment-method method-id="VISA_CHECKOUT">
        <name xml:lang="x-default">Visa Checkout</name>
        <enabled-flag>true</enabled-flag>
        <processor-id>CYBERSOURCE_CREDIT</processor-id>
        <countries>
            <country country-code="AR"/>
            <country country-code="AU"/>
            <country country-code="BR"/>
            <country country-code="CA"/>
            <country country-code="CL"/>
            <country country-code="CN"/>
            <country country-code="CO"/>
            <country country-code="HK"/>
            <country country-code="MX"/>
            <country country-code="MY"/>
            <country country-code="NZ"/>
            <country country-code="PE"/>
            <country country-code="SG"/>
            <country country-code="US"/>
            <country country-code="ZA"/>
        </countries>
    </payment-method>

    <payment-method method-id="SA_REDIRECT">
        <name xml:lang="x-default">Credit Card - Secure Acceptance Web/Mobile (Redirect)</name>
        <description xml:lang="x-default">Secure Acceptance Redirect</description>
        <enabled-flag>true</enabled-flag>
        <processor-id>CYBERSOURCE_CREDIT</processor-id>
    </payment-method>

    <payment-card card-type="Visa">
        <name xml:lang="x-default">Visa</name>
        <name xml:lang="fr-FR">Visa</name>
        <name xml:lang="it-IT">Visa</name>
        <name xml:lang="ja-JP">Visa</name>
        <name xml:lang="zh-CN">Visa</name>
        <enabled-flag>true</enabled-flag>
        <card-number-settings>
            <enable-checksum-verification>true</enable-checksum-verification>
            <number-length>
                <length>13</length>
                <length>16</length>
                <length>19</length>
            </number-length>
            <number-prefix>
                <prefix>4</prefix>
            </number-prefix>
        </card-number-settings>
        <security-code-length>3</security-code-length>
        <custom-attributes>
            <custom-attribute attribute-id="csEnablePayerAuthentication">true</custom-attribute>
        </custom-attributes>
    </payment-card>

    <payment-card card-type="Amex">
        <name xml:lang="x-default">American Express</name>
        <name xml:lang="fr-FR">American Express</name>
        <name xml:lang="it-IT">American Express</name>
        <name xml:lang="ja-JP">American Express</name>
        <name xml:lang="zh-CN">American Express（美国运通卡）</name>
        <enabled-flag>true</enabled-flag>
        <card-number-settings>
            <enable-checksum-verification>true</enable-checksum-verification>
            <number-length>
                <length>15</length>
            </number-length>
            <number-prefix>
                <prefix>34</prefix>
                <prefix>37</prefix>
            </number-prefix>
        </card-number-settings>
        <security-code-length>4</security-code-length>
        <custom-attributes>
            <custom-attribute attribute-id="csEnablePayerAuthentication">true</custom-attribute>
        </custom-attributes>
    </payment-card>

    <payment-card card-type="Discover">
        <name xml:lang="x-default">Discover</name>
        <name xml:lang="fr-FR">Discover</name>
        <name xml:lang="it-IT">Discover</name>
        <name xml:lang="ja-JP">Discover</name>
        <name xml:lang="zh-CN">Discover</name>
        <enabled-flag>true</enabled-flag>
        <card-number-settings>
            <enable-checksum-verification>true</enable-checksum-verification>
            <number-length>
                <length>16</length>
            </number-length>
            <number-prefix>
                <prefix>6011</prefix>
                <prefix>622126-622925</prefix>
                <prefix>644</prefix>
                <prefix>645</prefix>
                <prefix>646</prefix>
                <prefix>647</prefix>
                <prefix>648</prefix>
                <prefix>649</prefix>
                <prefix>65</prefix>
            </number-prefix>
        </card-number-settings>
        <security-code-length>3</security-code-length>
        <countries>
            <country country-code="US"/>
        </countries>
        <custom-attributes>
            <custom-attribute attribute-id="csEnablePayerAuthentication">true</custom-attribute>
        </custom-attributes>
    </payment-card>

    <payment-card card-type="MasterCard">
        <name xml:lang="x-default">MasterCard</name>
        <name xml:lang="fr-FR">Master Card</name>
        <name xml:lang="it-IT">Master Card</name>
        <name xml:lang="ja-JP">Master Card</name>
        <name xml:lang="zh-CN">Master Card（万事达卡）</name>
        <enabled-flag>true</enabled-flag>
        <card-number-settings>
            <enable-checksum-verification>true</enable-checksum-verification>
            <number-length>
                <length>16-19</length>
            </number-length>
            <number-prefix>
                <prefix>51-55</prefix>
            </number-prefix>
        </card-number-settings>
        <security-code-length>3</security-code-length>
        <custom-attributes>
            <custom-attribute attribute-id="csEnablePayerAuthentication">true</custom-attribute>
        </custom-attributes>
    </payment-card>

    <payment-card card-type="Maestro">
        <name xml:lang="x-default">Maestro</name>
        <enabled-flag>true</enabled-flag>
        <card-number-settings>
            <enable-checksum-verification>true</enable-checksum-verification>
            <number-length>
                <length>14-19</length>
            </number-length>
            <number-prefix>
                <prefix>5018</prefix>
                <prefix>5020</prefix>
                <prefix>5033</prefix>
                <prefix>5038</prefix>
                <prefix>5868</prefix>
                <prefix>5893</prefix>
                <prefix>6304</prefix>
                <prefix>6759</prefix>
                <prefix>6761</prefix>
                <prefix>6762</prefix>
                <prefix>6763</prefix>
            </number-prefix>
        </card-number-settings>
        <security-code-length>3</security-code-length>
        <custom-attributes>
            <custom-attribute attribute-id="csEnablePayerAuthentication">true</custom-attribute>
        </custom-attributes>
    </payment-card>
</payment-settings>
