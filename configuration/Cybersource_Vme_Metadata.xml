<?xml version="1.0" encoding="UTF-8"?>
<metadata xmlns="http://www.demandware.com/xml/impex/metadata/2006-10-31">  
    <type-extension type-id="SitePreferences">
        <custom-attribute-definitions>
            <attribute-definition attribute-id="VmeAcceptLogo">
                <display-name xml:lang="x-default">V.me by Visa Accept Logo</display-name>
                <description xml:lang="x-default">V.me by Visa Accept Logo</description>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
                <field-length>0</field-length>
                <default-value>https://static.v.me/img/vme_accpt_mark.png</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="VmeCustomerRepresentativeID">
                <display-name xml:lang="x-default">V.me by Visa Customer Representative ID (VMe Admin ID)</display-name>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
                <field-length>0</field-length>
            </attribute-definition>
            <attribute-definition attribute-id="VmeEnableLearnMoreLink">
                <display-name xml:lang="x-default">Show V.Me Learn More Link</display-name>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <default-value>true</default-value>
            </attribute-definition>            
            <attribute-definition attribute-id="VmeEnvironment">
                <display-name xml:lang="x-default">V.me by Visa Environment (Production, Sandbox)</display-name>
                <type>enum-of-string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <value-definitions>
                    <value-definition default="true">
                        <display xml:lang="x-default">Sandbox</display>
                        <value>Sandbox</value>
                    </value-definition>
                    <value-definition>
                        <display xml:lang="x-default">Production</display>
                        <value>Production</value>
                    </value-definition>
                </value-definitions>
            </attribute-definition>
            <attribute-definition attribute-id="VmeJavaScriptAPI">
                <display-name xml:lang="x-default">V.me by Visa JavaScript API - Production</display-name>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
                <field-length>0</field-length>
                <default-value>https://static.v.me//js/1/v-widgets.js</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="VmeMerchantLogo">
                <display-name xml:lang="x-default">V.me by Visa Merchant's logo</display-name>
                <type>image</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
            </attribute-definition>
            <attribute-definition attribute-id="VmeSandboxJavaScriptAPI">
                <display-name xml:lang="x-default">V.me by Visa JavaScript API - Sandbox</display-name>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
                <field-length>0</field-length>
                <default-value>https://sandbox-static.v.me//js/1/v-widgets.js</default-value>
            </attribute-definition>  
			<attribute-definition attribute-id="VmeDefaultFirstName">
                <display-name xml:lang="x-default">V.me by Visa Default First Name</display-name>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
                <field-length>0</field-length>
                <default-value>Valued</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="VmeDefaultLastName">
                <display-name xml:lang="x-default">V.me by Visa Default Last Name</display-name>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
                <field-length>0</field-length>
                <default-value>Customer</default-value>
            </attribute-definition>
			<attribute-definition attribute-id="VmeAmount">
				<display-name xml:lang="x-default">V.me by Visa Amount-Testing</display-name>
				<description xml:lang="x-default">V.me by Visa Amount for testing negative scenario i.e. $164</description>
				<type>double</type>
				<mandatory-flag>false</mandatory-flag>
				<externally-managed-flag>false</externally-managed-flag>
				<field-length>0</field-length>
			</attribute-definition>
			<attribute-definition attribute-id="VmeCheckoutButtonStyle">
                <display-name xml:lang="x-default">V.me by Visa Checkout Button Style Tag</display-name>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
                <field-length>0</field-length>
                <default-value>checkout</default-value>
            </attribute-definition>
			<attribute-definition attribute-id="VmePaymentButtonStyle">
                <display-name xml:lang="x-default">V.me by Visa Payment Button Style Tag</display-name>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
                <field-length>0</field-length>
                <default-value>payment</default-value>
            </attribute-definition>
			<attribute-definition attribute-id="VmeEnableVmeOnMiniCart">
				<display-name xml:lang="x-default">Enable V.Me Checkout button on Mini Cart</display-name>
				<type>boolean</type>
				<mandatory-flag>false</mandatory-flag>
				<externally-managed-flag>false</externally-managed-flag>
				<default-value>false</default-value>
			</attribute-definition>
			<attribute-definition attribute-id="VmeServersideClickjackingEnabled">
				<display-name xml:lang="x-default">Enable V.Me Server side clickjacking</display-name>
				<type>boolean</type>
				<mandatory-flag>false</mandatory-flag>
				<externally-managed-flag>false</externally-managed-flag>
				<default-value>false</default-value>
			</attribute-definition>				
        </custom-attribute-definitions>
        <group-definitions>
            <attribute-group group-id="CYBS_VME">
                <display-name xml:lang="x-default">V.me By Visa</display-name>
                <attribute attribute-id="VmeSandboxJavaScriptAPI"/>
                <attribute attribute-id="VmeJavaScriptAPI"/>                
                <attribute attribute-id="VmeEnvironment"/>
                <attribute attribute-id="VmeCustomerRepresentativeID"/>
                <attribute attribute-id="VmeMerchantLogo"/>
                <attribute attribute-id="VmeAcceptLogo"/>
                <attribute attribute-id="VmeEnableLearnMoreLink"/>
				<attribute attribute-id="VmeDefaultLastName"/>
                <attribute attribute-id="VmeDefaultFirstName"/>
				<attribute attribute-id="VmeAmount"/>
                <attribute attribute-id="VmeCheckoutButtonStyle"/>
                <attribute attribute-id="VmePaymentButtonStyle"/>
				<attribute attribute-id="VmeEnableVmeOnMiniCart"/>
				<attribute attribute-id="VmeServersideClickjackingEnabled"/>				
            </attribute-group>
        </group-definitions>
    </type-extension>
</metadata>