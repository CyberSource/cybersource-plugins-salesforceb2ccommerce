/**
*	CreateCyberSourcePaymentCardObject_UserData.ds
*	This script creates the PaymentCard user data object and set it to CyberSourceCard output object.
*
*	@input formType : String
*	@output CyberSourceCard : Object
*/
var CardHelper = require('int_cybersource/cartridge/scripts/helper/CardHelper');

function execute( pdict : PipelineDictionary ) : Number
{
	pdict.CyberSourceCard = CardHelper.CreateCybersourcePaymentCardObject(pdict.formType).card;
	
    return PIPELET_NEXT;
}
