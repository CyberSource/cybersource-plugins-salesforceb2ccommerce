    <payment-card card-type="JCB">
        <name xml:lang="x-default">JCB Card</name>
        <description xml:lang="x-default">JCB</description>
        <enabled-flag>false</enabled-flag>
        <card-number-settings>
            <enable-checksum-verification>true</enable-checksum-verification>
            <number-length>
                <length>16-19</length>
            </number-length>
            <number-prefix/>
        </card-number-settings>
        <security-code-length>3</security-code-length>
        <countries>
            <country country-code="CA"/>
            <country country-code="DE"/>
            <country country-code="US"/>
        </countries>
        <customer-groups>
            <customer-group group-id="Everyone"/>
        </customer-groups>
        <custom-attributes>
            <custom-attribute attribute-id="csEnablePayerAuthentication">true</custom-attribute>
        </custom-attributes>
    </payment-card>

